{
  "name": "we-core",
  "version": "1.8.20",
  "description": "We.js is a node.js framework for build real time applications, sites or blogs!",
  "homepage": "https://wejs.org",
  "main": "./lib/index.js",
  "scripts": {
    "test": "NODE_ENV=test LOG_LV=info ./node_modules/.bin/mocha test/bootstrap.js test/**/*.test.js -b ",
    "verbose-test": "NODE_ENV=test LOG_LV=verbose ./node_modules/.bin/mocha test/bootstrap.js test/**/*.test.js -b ",
    "coverage": "NODE_ENV=test LOG_LV=info node_modules/.bin/istanbul cover -x src node_modules/.bin/_mocha -- -u exports -R spec test/bootstrap.js test/**/*.test.js",
    "preversion": "npm run compile && npm test",
    "postversion": "git push",
    "compile": "babel src --out-dir lib",
    "cw": "babel src --out-dir lib -watch "
  },
  "keywords": [
    "we.js",
    "wejs",
    "plugin",
    "wejs-plugin",
    "framework"
  ],
  "repository": "wejs/we-plugin-core",
  "files": [
    "files",
    "lib",
    "locales",
    "server",
    "install.js",
    "plugin.js"
  ],
  "author": "Alberto Souza <contato@albertosouza.net>",
  "license": "MIT",
  "dependencies": {
    "async": "2.1.1",
    "body-parser": "1.15.2",
    "compression": "1.6.2",
    "connect-flash": "0.1.1",
    "cookie-parser": "1.4.3",
    "cors": "2.8.1",
    "express": "4.14.0",
    "express-session": "1.14.1",
    "handlebars": "4.0.5",
    "i18n": "0.8.3",
    "lodash": "4.16.4",
    "mime": "1.3.4",
    "mkdirp": "0.5.1",
    "moment": "2.15.1",
    "morgan": "1.7.0",
    "node-uuid": "1.4.7",
    "request": "2.75.0",
    "sanitize-html": "1.13.0",
    "sequelize": "3.24.3",
    "serve-favicon": "~2.3.0",
    "simple-hooks-callback": "1.0.0",
    "string": "3.3.3",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-preset-es2015": "^6.16.0",
    "chance": "^1.0.4",
    "express-mysql-session": "1.2.0",
    "istanbul": "0.4.5",
    "mocha": "3.1.2",
    "mysql": "2.11.1",
    "ncp": "^2.0.0",
    "pg": "^6.1.0",
    "pg-hstore": "^2.3.2",
    "rimraf": "2.5.4",
    "sinon": "1.17.6",
    "supertest": "2.0.0",
    "we-test-tools": "wejs/we-test-tools"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "wejs": {
    "plugins": {
      "we-plugin-post": true
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "array-includes"
    ]
  }
}
