<ul class="{{menu.class}}">
  {{#each menu.links}}
    {{#each this}}
      {{#if links}}
<li class="dropdown">
  <a aria-expanded="false" aria-haspopup="true" role="button" data-toggle="dropdown" class="dropdown-toggle" href="#"> {{{text}}} <span class="caret"></span></a>
  <ul class="dropdown-menu">
    {{#each links}}
      {{#if text}}
        <li>
          <a {{#if href}}href="{{href}}"{{/if}} {{#if title}}title="{{title}}"{{/if}} {{#if class}}class="{{class}}"{{/if}} style="{{style}}" target="{{target}}" rel="{{rel}}">
            {{{text}}}
          </a>
        </li>
      {{/if}}
    {{/each}}
  </ul>
</li>
      {{else}}
<li>
  <a {{#if href}}href="{{href}}"{{/if}} {{#if title}}title="{{title}}"{{/if}} {{#if class}}class="{{class}}"{{/if}} style="{{style}}" target="{{target}}" rel="{{rel}}">
    {{{text}}}
  </a>
</li>
      {{/if}}
    {{/each}}
  {{/each}}
</ul>